{
    "name": "netsells/attribute-finder",
    "description": "A simple package that allows you to get all classes that have a given attribute in a specified directory",
    "type": "library",
    "license": "MIT",
     "keywords": [
        "attributes",
        "search"
    ],
    "authors": [
        {
            "name": "Adam Hutchison",
            "email": "adam.hutchison@netsells.co.uk"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/finder": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "Netsells\\AttributeFinder\\": "src",
            "Netsells\\AttributeFinder\\Tests\\": "tests"
        }
    }
}
